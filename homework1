/* Вариант 21 */ 
#include <stdio.h>   
void clrscr(void)
{
	char a[80];
	printf("\033[2J");
	printf("\033[0;0f");
}

int main() 
{
	clrscr();
	int k1, k2, k3;
	float MM, NN, v, u, w;
	printf("k1="); scanf("%d",&k1);
	printf("\033[31m");
	printf("k2="); scanf("%d",&k2);
	printf("\033[32m");
	printf("k3="); scanf("%d",&k3);
	printf("\033[33m");
	printf("m="); scanf("%f",&MM);
	printf("\033[34m");
	printf("n="); scanf("%f",&NN);
	printf("\033[35m");
	clrscr();
	if (k3+k2 != 0)
	{
		if (k2 != 0)
		{
			w=6/7 + k1/(k2+k3) + (MM + NN)/k2;
			printf("\033[36m");
			printf("w=%f\n",w);
		}
		else
			printf("infinity error in W\n");
	}
	else
		printf("infinity error in W\n");
	if (k3 != 0)
	{
		if (MM + k2/k3 != 0)
		{
			u=(10e-2 + 1/k3)/10e2 + (MM + k1/k3)/(MM + k2/k3);
			printf("u=%f\n",u);
		}
		else printf("infinity error in U\n");
	}
	else
		printf("infinity error in U\n");
	if (k3 != -13)
	{
		if (k3 != -20)
		{
			v=(((13-k1)/(13+k3))-((10e-7 +k1)/(20 + k3)))*10e-3;
			printf("v=%f\n",v);
		}
		else printf("infinite error in V\n");
	}
	else printf("infinite error in V\n");
	

}
